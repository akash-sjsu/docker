{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {

          "image": "ajag408/bankapi",
          "memory": 250,
          "name": "app",

          "portMappings": [
              {
                  "hostPort": 3000,
                  "containerPort": 3000
              }
          ],
          "links": [
            "mongo"
          ]
      },
        {
            "environment": [
                {
                    "name": "MONGODB_DATABASE",
                    "value": "bankapi"
                }
            ],
            "image": "mongo",
            "memory": 128,
            "name": "mongo"
        }

    ]
}
